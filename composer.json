{
    "name": "hypervel/cache",
    "type": "library",
    "description": "The cache package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "cache",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Cache\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "require": {
        "php": "^8.2",
        "hyperf/config": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "laravel/serializable-closure": "^1.3",
        "psr/simple-cache": "^3.0"
    },
    "suggest": {
        "hyperf/redis": "Required to use redis driver. (~3.1.0)",
        "hypervel/cache": "Required to use file lock. (~0.1.0)"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\Cache\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    }
}
